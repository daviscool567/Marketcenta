  async doLogout() {\r\n      const auth_token = await this.auth_token\r\n      axios\r\n        .get(this.dynamic_auth_route(\"auth/logout\"), {\r\n          headers: {\r\n            authorization: `Bearer ${auth_token}`,\r\n          },\r\n        })\r\n        .then((res) => {\r\n          if (res.data.status == \"error\") {\r\n            this.openToast(res.data.message, 'danger');\r\n          } else {\r\n            this.openToast(\"Logout Successful\", 'success');\r\n            this.destroyToken();\r\n            this.getAuthData();\r\n            this.$router.push({ name: \"Home\" });\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          this.openToast(\"Network error. Please, try again.\", 'danger');\r\n          this.tokenExpired(error)\r\n        });\r\n    },\r\n    menuNavigation(url){\r\n      menuController.close(\"app-menu\");\r\n      this.$router.push(url);\r\n    }\r\n  }\r\n});\r\n</script>\r\n<style scoped>\r\n.main a {\r\n  color: #555555 !important;\r\n  font-size: 14px;\r\n  padding-top: 10px;\r\n  padding-bottom: 10px;\r\n}\r\n.main a:hover {\r\n  text-decoration: none !important;\r\n  background-color: #fff1e8;\r\n}\r\n</style>"]}]}