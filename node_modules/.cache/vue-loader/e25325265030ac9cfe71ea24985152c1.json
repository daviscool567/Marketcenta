ken = await this.auth_token\n      axios\n        .get(this.dynamic_auth_route(\"auth/logout\"), {\n          headers: {\n            authorization: `Bearer ${auth_token}`,\n          },\n        })\n        .then((res) => {\n          if (res.data.status == \"error\") {\n            this.openToast(res.data.message, 'danger');\n          } else {\n            this.openToast(\"Logout Successful\", 'success');\n            this.destroyToken();\n            this.getAuthData();\n            this.$router.push({ name: \"Home\" });\n          }\n        })\n        .catch((error) => {\n          this.openToast(\"Network error. Please, try again.\", 'danger');\n          this.tokenExpired(error)\n        });\n    },\n    menuNavigation(url){\n      menuController.close(\"app-menu\");\n      this.$router.push(url);\n    }\n  }\n});\n</script>\n<style scoped>\n.main a {\n  color: #555555 !important;\n  font-size: 14px;\n  padding-top: 10px;\n  padding-bottom: 10px;\n}\n.main a:hover {\n  text-decoration: none !important;\n  background-color: #fff1e8;\n}\n</style>"]}]}