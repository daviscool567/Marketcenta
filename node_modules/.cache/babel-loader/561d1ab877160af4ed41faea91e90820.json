oBA,eAAe;AACbC,EAAAA,UAAU,EAAE,IADC;AAEbpE,EAAAA,KAFa;AAGbyD,EAAAA,SAHa;AAIbtC,EAAAA,OAJa;AAKbX,EAAAA;AALa,CAAf","sourcesContent":["import axios from \"axios\";\r\nimport { Storage } from '@ionic/storage';\r\n\r\nconst store = new Storage();\r\nstore.create();\r\n\r\nconst state = {\r\n  authToken: store.get(\"clientToken\") || null,\r\n  user: {},\r\n  all_categories: [],\r\n  sub_categories: [],\r\n  // all_products: { data: [] },\r\n  all_carts: [],\r\n  carts_total: 0,\r\n};\r\n\r\nconst getters = {\r\n  loggedIn: (state) => state.authToken != null,\r\n  auth_token: (state) => state.authToken,\r\n  auth_user: (state) => state.user,\r\n  allCategories: (state) => state.all_categories.product_category,\r\n  subCategories: (state) => state.sub_categories,\r\n  // allProducts: (state) => state.all_products,\r\n  carts_data: (state) => state.all_carts,\r\n  cart_total: (state) => state.carts_total,\r\n  cartCount: (state) => state.all_carts.length,\r\n};\r\n\r\nconst actions = {\r\n  setAuthData({ commit }, resp) {\r\n    // const store = new Storage();\r\n    // store.create();\r\n    store.set(\"clientToken\", resp.access_token);\r\n    store.set(\"clientDetails\", JSON.stringify(resp.user));\r\n    commit(\"setToken\", resp.access_token);\r\n  },\r\n  getAuthData({ commit }) {\r\n    // const store = new Storage();\r\n    // store.create();\r\n    const text = store.get(\"clientDetails\");\r\n    const clientDetails = JSON.parse(text);\r\n    commit(\"fetchData\", clientDetails);\r\n  },\r\n  destroyToken({ commit }) {\r\n    // const store = new Storage();\r\n    // store.create();\r\n    store.remove(\"clientToken\");\r\n    store.remove(\"clientDetails\");\r\n    commit(\"removeToken\");\r\n  },\r\n  async getCategories({ commit }, url) {\r\n    const response = await axios.post(url, {\r\n      not_paginated: true,\r\n      client: true,\r\n    });\r\n\r\n    commit(\"setCategories\", response.data);\r\n  },\r\n  async getSubCategories({ commit }, id) {\r\n    const response = await axios.post(\r\n      \"http://127.0.0.1:8000/api/client/products/subcategories/fetch\",\r\n      { no_paginate: true, catId: id }\r\n    );\r\n\r\n    commit(\"setSubCategories\", response.data.product_sub_category);\r\n  },\r\n  // async getProducts({ commit }, filters = null, page = 1) {\r\n  //   const response = await axios.post(\r\n  //     \"http://127.0.0.1:8000/api/client/products/fetch?page=\" + page,\r\n  //     { filters: filters, client_pag: true }\r\n  //   );\r\n\r\n  //   commit(\"setAllProducts\", response.data.data);\r\n  // },\r\n\r\n  showUser({ commit }, userId) {\r\n    const token = store.get(\"clientToken\");\r\n    axios\r\n      .get(`http://127.0.0.1:8000/api/user/fetch/${userId}`, {\r\n        headers: {\r\n          authorization: `Bearer ${token}`,\r\n        },\r\n      })\r\n      .then((res) => {\r\n        store.set(\"clientDetails\", JSON.stringify(res.data));\r\n        commit(\"updateUser\", res.data);\r\n      });\r\n  },\r\n  getCarts({ commit }, id) {\r\n    let auth_token = store.get(\"clientToken\");\r\n    axios\r\n      .post(\r\n        \"http://127.0.0.1:8000/api/client/products/carts/fetch\",\r\n        { id: id },\r\n        {\r\n          headers: {\r\n            authorization: `Bearer ${auth_token}`,\r\n          },\r\n        }\r\n      )\r\n      .then((response) => {\r\n        commit(\"setCarts\", response.data.mssg[0]);\r\n        commit(\"setCartsTotal\", response.data.mssg[1]);\r\n      })\r\n      .catch((err) => {\r\n        this.tokenExpired(err);\r\n      });\r\n  },\r\n};\r\n\r\nconst mutations = {\r\n  setToken: (state, token) => {\r\n    state.authToken = token;\r\n  },\r\n  fetchData(state, authData) {\r\n    state.user = authData;\r\n  },\r\n  removeToken(state) {\r\n    (state.authToken = null), (state.user = {});\r\n  },\r\n  setCategories: (state, data) => (state.all_categories = data),\r\n  setSubCategories: (state, data) => (state.sub_categories = data),\r\n  // setAllProducts: (state, data) => (state.all_products = data),\r\n  setCarts: (state, data) => (state.all_carts = data),\r\n  setCartsTotal: (state, data) => (state.carts_total = data),\r\n  updateUser: (state, userData) => {\r\n    state.user = userData;\r\n  },\r\n};\r\n\r\nexport default {\r\n  namespaced: true,\r\n  state,\r\n  mutations,\r\n  actions,\r\n  getters,\r\n};\r\n"]}]}