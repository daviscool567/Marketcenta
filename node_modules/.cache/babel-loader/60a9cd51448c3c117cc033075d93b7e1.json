;;AAKPC,IAAAA,kBAAkB,CAACD,MAAD,EAAS;AACzB;AACA,aAAQ,6BAA4BA,MAAO,EAA3C;AACD,KARM;;AASP,UAAME,SAAN,CAAgBC,OAAhB,EAAyBC,KAAzB,EAAgC;AAC9B,YAAMC,KAAK,GAAG,MAAMZ,eAAe,CAACa,MAAhB,CAAuB;AACzCH,QAAAA,OAAO,EAAEA,OADgC;AAEzCI,QAAAA,QAAQ,EAAE,IAF+B;AAGzCH,QAAAA,KAAK,EAAEA,KAHkC;AAIzCI,QAAAA,QAAQ,EAAE;AAJ+B,OAAvB,CAApB;AAMA,aAAOH,KAAK,CAACI,OAAN,EAAP;AACD,KAjBM;;AAkBP,UAAMC,cAAN,CAAqBP,OAArB,EAA8B;AAC5B,YAAMQ,OAAO,GAAG,MAAMjB,iBAAiB,CACpCY,MADmB,CACZ;AACNM,QAAAA,QAAQ,EAAE,eADJ;AAENT,QAAAA,OAAO,EAAG,GAAEA,OAAQ,kBAFd;AAGNU,QAAAA,OAAO,EAAE,UAHH,CAIN;;AAJM,OADY,CAAtB;AAOA,YAAMF,OAAO,CAACF,OAAR,EAAN;AACD,KA3BM;;AA4BP,UAAMK,cAAN,GAAsB;AACpB,YAAMpB,iBAAiB,CAACqB,OAAlB,EAAN;AACD,KA9BM;;AA+BPC,IAAAA,YAAY,CAACC,KAAD,EAAQ;AAClB,UACEA,KAAK,CAACC,QAAN,CAAeC,MAAf,IAAyB,GAAzB,IACAF,KAAK,CAACC,QAAN,CAAeE,UAAf,IAA6B,cAF/B,EAGE;AACAC,QAAAA,YAAY,CAACC,UAAb,CAAwB,aAAxB;AACAD,QAAAA,YAAY,CAACC,UAAb,CAAwB,eAAxB;AACA,aAAKC,OAAL,CAAaC,IAAb,CAAkB;AAAEC,UAAAA,IAAI,EAAE;AAAR,SAAlB;AACD;AACF;;AAxCM;AADD,CAAV;AA6CFnC,MAAM,CAACoC,OAAP,GAAiBC,IAAjB,CAAsB,MAAM;AAC1BhC,EAAAA,GAAG,CAACiC,KAAJ,CAAU,MAAV;AACD,CAFD","sourcesContent":["import { createApp } from 'vue'\nimport App from './App.vue'\nimport router from './router';\n\nimport { IonicVue } from '@ionic/vue';\n\n/* Core CSS required for Ionic components to work properly */\nimport '@ionic/vue/css/core.css';\n\n/* Basic CSS for apps built with Ionic */\nimport '@ionic/vue/css/normalize.css';\nimport '@ionic/vue/css/structure.css';\nimport '@ionic/vue/css/typography.css';\n\n/* Optional CSS utils that can be commented out */\nimport '@ionic/vue/css/padding.css';\nimport '@ionic/vue/css/float-elements.css';\nimport '@ionic/vue/css/text-alignment.css';\nimport '@ionic/vue/css/text-transformation.css';\nimport '@ionic/vue/css/flex-utils.css';\nimport '@ionic/vue/css/display.css';\n\n/* Theme variables */\nimport './theme/variables.css';\n\nimport store from \"./store/index\";\nimport { toastController, loadingController } from '@ionic/vue';\n\nconst app = createApp(App)\n  .use(IonicVue)\n  .use(router)\n  .use(store)\n  \n  app.mixin({\n    methods: {\n      dynamic_route(endUrl) {\n        return `http://127.0.0.1:8000/api/client/${endUrl}`;\n        // return `https://api.diimtech.com/api/client/${endUrl}`;\n      },\n      dynamic_auth_route(endUrl) {\n        // return `https://api.diimtech.com/api/${endUrl}`;\n        return `http://127.0.0.1:8000/api/${endUrl}`;\n      },\n      async openToast(message, color) {\n        const toast = await toastController.create({\n          message: message,\n          duration: 3000,\n          color: color,\n          position: 'top'\n        });\n        return toast.present();\n      },\n      async presentLoading(message) {\n        const loading = await loadingController\n          .create({\n            cssClass: 'loading-class',\n            message: `${message}. Please wait...`,\n            spinner: 'circular',\n            // duration: this.timeout,\n          });\n        await loading.present();\n      },\n      async dismissLoading(){\n        await loadingController.dismiss()\n      },\n      tokenExpired(error) {\n        if (\n          error.response.status == 401 &&\n          error.response.statusText == \"Unauthorized\"\n        ) {\n          localStorage.removeItem(\"clientToken\");\n          localStorage.removeItem(\"clientDetails\");\n          this.$router.push({ name: \"Home\" });\n        }\n      },\n    }\n  })\n\nrouter.isReady().then(() => {\n  app.mount('#app');\n});"]}]}